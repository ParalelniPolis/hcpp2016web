<script>
$('.page-scroll').on('click', function(event) {
  var hash = $(this).attr('href').replace('/', '');

  if($(hash).length > 0) {
    event.preventDefault();

    $('html, body').animate({
      scrollTop: $(hash).offset().top
    }, 1000, function() {
      window.location.hash = hash;
    });
  }
});

$('.see-more').on('click', function() {
  $('.container-expand').slideDown();
  $('.see-more').fadeOut();
});
</script>

